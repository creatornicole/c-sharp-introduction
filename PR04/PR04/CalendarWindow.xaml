<Window x:Class="PR04.CalendarWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PR04"
        mc:Ignorable="d"
        Title="CalendarWindow" Height="450" Width="800">
    <Grid>
        <Calendar Name="calendarView" HorizontalAlignment="Center" VerticalAlignment="Center" SelectedDatesChanged="Calendar_Selected_Date_Changed"/>
        <Button Name="backBtn" Content="Back" HorizontalAlignment="Left" Margin="517,97,0,0" VerticalAlignment="Top" Click="Button_Click_Back_To_Page"/>
        <TextBlock Name="infoBox" HorizontalAlignment="Center" Margin="0,94,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" RenderTransformOrigin="0.335,2.133" Width="180"/>

    </Grid>
</Window>
